{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "allowSyntheticDefaultImports": true, 
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": false,
    "removeComments": false,
    "strict": true,
    "outDir": "build",
    "lib": ["es6", "es7", "dom"]
  },
  "exclude": [
    "node_modules"
  ]
}
